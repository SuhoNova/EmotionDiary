    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:local="clr-namespace:EmotionDiary"
    x:Class="EmotionDiary.MainPage">
    <NavigationPage Title="Take Selfie!">
        <x:Arguments>
            <local:CognitiveEmotion />
        </x:Arguments>
    </NavigationPage>
    <NavigationPage Title="View Emotion History">
        <x:Arguments>
            <local:AzureTable />
        </x:Arguments>
    </NavigationPage>
</TabbedPage>
